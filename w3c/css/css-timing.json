{
  "spec": {
    "title": "CSS Timing Functions Level 1",
    "url": "https://drafts.csswg.org/css-timing/"
  },
  "properties": {},
  "descriptors": {},
  "valuespaces": {
    "<cubic-bezier()>": {
      "value": "cubic-bezier(<number>, <number>, <number>, <number>)"
    },
    "<steps()>": {
      "value": "steps(<integer>[, <step-position> ]?)"
    },
    "<timing-function>": {
      "value": "linear | <cubic-bezier-timing-function> | <step-timing-function>"
    },
    "<cubic-bezier-timing-function>": {
      "value": "ease | ease-in | ease-out | ease-in-out | cubic-bezier(<number>, <number>, <number>, <number>)"
    },
    "<step-timing-function>": {
      "value": "step-start | step-end | steps(<integer>[, <step-position>]?)"
    },
    "<step-position>": {
      "value": "jump-start | jump-end | jump-none | jump-both | start | end"
    }
  }
}
